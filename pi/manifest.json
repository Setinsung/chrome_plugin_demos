{
    "name": "pi",
    "description": "package install and test in devtool",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "content.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}